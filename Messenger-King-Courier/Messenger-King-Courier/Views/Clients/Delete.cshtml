            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_IDNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Surname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Cellnumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Address)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Client_Tellnum)
            </th>
        </tr>
        <tr class="info">
