            </td>
            <td>
                @Html.DisplayFor(model => model.Client_Cellnumber)
            </td>
            <td>
                @Html.DisplayFor(model => model.Client_Address)
            </td>
            <td>
                @Html.DisplayFor(model => model.Client_Email)
            </td>
            <td>
                @Html.DisplayFor(model => model.Client_Tellnum)
            </td>
        </tr>
    </table>
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-primary" /> |
            <a href='@Url.Action("Index", "Clients")' class="btn btn-success">Back to List</a>|
        </div>
    }
