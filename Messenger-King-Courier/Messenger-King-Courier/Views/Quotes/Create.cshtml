                @Html.EditorFor(model => model.Quote_Height, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Quote Height" } })
                @Html.ValidationMessageFor(model => model.Quote_Height, "", new { @class = "text-danger" })
                <div class="help-block with-errors"></div>
