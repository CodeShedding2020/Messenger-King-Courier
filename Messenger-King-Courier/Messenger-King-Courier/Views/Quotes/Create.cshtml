                                        <li class="divider"></li>
                                        <li><a @Html.ActionLink("Register", "Register", "Account") </a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!--/.navbar-collapse-->
                    <!--/.navbar-->
                </div>
            </nav>
        </div>
    </div>
    <!-- //header -->
    <center>
        <h3 class="heading" style="color:white">Quote</h3>
    </center>

    <div class="content-agileits">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <hr />
            @Html.HiddenFor(model => model.Client_ID)
            @Html.HiddenFor(model => model.Rate_ID)
            <div class="form-group w3layouts w3 w3l">
                <label for="firstname" class="control-label">Quote Pickup Address: </label>
                @Html.EditorFor(model => model.Quote_PickupAddress, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Quote Pickup Address" } })
                @Html.ValidationMessageFor(model => model.Quote_PickupAddress, "", new { @class = "text-danger" })
                <div class="help-block with-errors"></div>
            </div>

            <div class="form-group w3layouts w3 w3l">
                <label for="firstname" class="control-label">Quote Delivery Address: </label>
                @Html.EditorFor(model => model.Quote_DeliveryAddress, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Quote Delivery Address" } })
