                <div class="help-block with-errors"></div>
            </div>

            <div class="form-group w3layouts w3 w3l">
                <label for="firstname" class="control-label">Quote Weight : </label>
                @Html.EditorFor(model => model.Quote_Weight, new { htmlAttributes = new { @class = "form-control", @placeholder = "Enter Quote Weight" } })
                @Html.ValidationMessageFor(model => model.Quote_Weight, "", new { @class = "text-danger" })
