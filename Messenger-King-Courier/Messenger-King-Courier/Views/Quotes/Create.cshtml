                                <a @Html.ActionLink("Orders", "Index", "Home") <b></a></b><br>
                                <a @Html.ActionLink("-Get Quote", "Create", "Quotes") </a><br>
                                <a @Html.ActionLink("-Manage Orders", "Create", "Orders") </a>
                            </li>
                            <li>
