            </div>

            <center>
                <div class="form-group">
                    <div class="col-md-offset-2 col-md-10">
                        <input type="submit" value="Create" class="btn btn-primary" />
                    </div>
                </div>
            </center>
            <center>
                <a href='@Url.Action("Index", "Quotes")' class="btn btn-primary">Back to List</a>|
            </center>
        }
    </div>


        <!-- footer -->
