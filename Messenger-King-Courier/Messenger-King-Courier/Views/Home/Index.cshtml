                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <nav>
                            <ul class="nav navbar-nav">
                                <li><a class="active" @Html.ActionLink("Home", "Index", "Home") </a></li>
                                <li><a @Html.ActionLink("Booking", "Create", "Bookings") </a></li>
                                <li><a @Html.ActionLink("Tracking", "Create", "Trackings") </a></li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Orders <b class="caret"></b></a>
