                                        <li><a @Html.Partial("_LoginPartial") </a></li>
                                        <li class="divider"></li>
                                        <li><a @Html.ActionLink("Register", "Register", "Account") </a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!--/.navbar-collapse-->
                    <!--/.navbar-->
                </div>
            </nav>
        </div>
    </div>
