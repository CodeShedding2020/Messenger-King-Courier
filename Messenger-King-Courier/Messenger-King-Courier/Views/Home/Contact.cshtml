    <link href="~/LayOuts/FullSite Files/css/bootstrap.css" rel="stylesheet" type="text/css" media="all" /><!-- bootstrap css -->
    <link href="~/LayOuts/FullSite Files/css/contact.css" rel="stylesheet" type="text/css" media="all" /><!-- contact css -->
    <link href="~/LayOuts/FullSite Files/css/style.css" rel="stylesheet" type="text/css" media="all" />

    <link href="~/LayOuts/FullSite Files/css/font-awesome.css" rel="stylesheet"> <!-- fontawesome css -->
    <!--fonts-->
    <link href="//fonts.googleapis.com/css?family=Open+Sans:400,600,700" rel="stylesheet" type="text/css">
    <link href="//fonts.googleapis.com/css?family=Montserrat:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;subset=latin-ext,vietnamese" rel="stylesheet">
    <!--//fonts-->

</head>
<body>

    <!-- header -->
    <div class="header">
        <div class="content white">
            <nav class="navbar navbar-default" role="navigation">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="index.html">
                            <h1>@Html.ActionLink("Messenger King", "Index", "Home")</h1>
                        </a>
                    </div>
                    <!--/.navbar-header-->
                    <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                        <nav>
                            <ul class="nav navbar-nav">
                                <li><a class="active" @Html.ActionLink("Home", "Index", "Home") </a></li>
                                <li><a @Html.ActionLink("Booking", "Create", "Bookings") </a></li>
                                <li><a @Html.ActionLink("Tracking", "Create", "Trackings") </a></li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Orders <b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li><a @Html.ActionLink("Get Quote", "Create", "Quotes") </a></li>
                                        <li class="divider"></li>
                                        <li><a @Html.ActionLink("Manage Orders", "Create", "Orders") </a></li>
                                    </ul>
                                </li>
                                <li><a @Html.ActionLink("Contact Us", "Contact", "Home") </a></li>
                                <li class="dropdown">
                                    <a href="#" class="dropdown-toggle" data-toggle="dropdown">Profile <b class="caret"></b></a>
                                    <ul class="dropdown-menu">
                                        <li><a @Html.Partial("_LoginPartial") </a></li>
                                        <li class="divider"></li>
                                        <li><a @Html.ActionLink("Register", "Register", "Account") </a></li>
                                    </ul>
                                </li>
                            </ul>
                        </nav>
                    </div>
                    <!--/.navbar-collapse-->
                    <!--/.navbar-->
                </div>
            </nav>
        </div>
    </div>
    <!-- //header -->
    <!-- Contact -->
    <div class="contact-section" id="contact">
        <div class="container">
            <h3 class="heading">Contact Us</h3>
            <div class="col-md-4 address-left">
                <div class="address-grid">
                    <h4>Contact <span>Info</span></h4>
                    <div class="address-grids">
                        <span class="fa fa-volume-control-phone" aria-hidden="true"></span>
                        <div class="contact-right">
                            <p>Telephone </p><span>+031 566 1604</span>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="address-grids">
                        <span class="fa fa-envelope-o" aria-hidden="true"></span>
                        <div class="contact-right">
                            <p>Mail </p><a href="mailto:info@messengerking.co.za">info@messengerking.co.za</a>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="address-grids">
                        <span class="fa fa-map-marker" aria-hidden="true"></span>
                        <div class="contact-right">
                            <p>Location</p><span>32 Marshall Drive, Mount Edgecomb, Durban, 4302</span>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                </div>
            </div>
            <div class="col-md-8 map">
                <iframe src="https://www.google.com/maps/embed?pb=!1m19!1m8!1m3!1d3465.391355409538!2d31.0322233!3d-29.708421!3m2!1i1024!2i768!4f13.1!4m8!3e2!4m0!4m5!1s0x1ef7050033b56acd%3A0x37011e94403c560a!2s32%20Marshall%20Dr%2C%20Mount%20Edgecombe%2C%204302!3m2!1d-29.708420999999998!2d31.034412!5e0!3m2!1sen!2sza!4v1594661317571!5m2!1sen!2sza" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
            </div>

        </div>
    </div>
    <!--// Contact -->
    <!-- footer -->
    <div class="footer">
        <div class="container">
            <div class="w3_agile_footer_grids">
                <ul class="col-md-3 col-xs-6 w3_agile_footer_grid right">
                    <h3>Opening Hours</h3>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Monday <span>8:00 - 17:00</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Tuesday <span>8:00 - 17:00</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Wednesday <span>8:00 - 17:00</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Thursday <span>8:00 - 17:00</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Friday <span>8:00 - 17:00</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Weekends <span>Closed</span></li>
                    <li><span class="fa fa-clock-o" aria-hidden="true"></span>Public Holidays <span>Closed</span></li>
                </ul>
                <div class="col-md-3 col-xs-6 w3_agile_footer_grid">
                    <h3>Navigation</h3>
                    <ul class="agileits_w3layouts_footer_grid_list">
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Home", "Index", "Home") </a>
                        </li>
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Booking", "Create", "Bookings") </a>
                        </li>
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Tracking", "Create", "Trackings") </a>
                        </li>
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Orders", "Index", "Home") <b></a></b><br>
                            <a @Html.ActionLink("-Get Quote", "Create", "Quotes") </a><br>
                            <a @Html.ActionLink("-Manage Orders", "Create", "Orders") </a>
                        </li>
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Contact Us", "Contact", "Home") </a>
                        </li>
                        <li>
                            <span class="fa fa-angle-double-right" aria-hidden="true"></span>
                            <a @Html.ActionLink("Profile", "Index", "Home") <b></a></b><br>
                            <a @Html.ActionLink("-Login", "Login", "Account") </a><br>
                            <a @Html.ActionLink("-Register", "Register", "Account") </a>
                        </li>
                    </ul>
                </div>
                <div class="col-md-3 col-xs-6 w3ls_address_mail_footer_grids w3_agile_footer_grid">
                    <h3>Contact Us</h3>
                    <div class="w3ls_footer_grid_left">
                        <div class="w3l-icon">
                            <span class="fa fa-map-marker" aria-hidden="true"></span>
                        </div>
                        <p>
                            32 Marshall Drive, Mount Edgecomb, Durban, 4302
                        </p>
                        <div class="clearfix"></div>
                    </div>
                    <div class="w3ls_footer_grid_left">
                        <div class="w3l-icon">
                            <span class="fa fa-phone" aria-hidden="true"></span>
                        </div>
                        <p>
                            +031 566 1604
                        </p>
                        <div class="clearfix"></div>
                    </div>
                    <div class="w3ls_footer_grid_left">
                        <div class="w3l-icon">
                            <span class="fa fa-envelope-o" aria-hidden="true"></span>
                        </div>
                        <p>
                            <a href="mailto:info@messengerking.co.za">info@messengerking.co.za</a>
                        </p>
                        <div class="clearfix"></div>
                    </div>
                </div>

                <div class="clearfix"> </div>
            </div>
            <div class="agileinfo_copyright">
                <p>
                    © 2020 Messenger King. All Rights Reserved | Design by CodeShedding Inc.</a>
                </p>
            </div>
        </div>
    </div>
    <!-- //footer -->
    <!-- Supportive js -->
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/jquery-2.1.4.min.js"></script>
    <!-- //Supportive js -->
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/bootstrap.js"></script><!-- bootstrap js file -->
    <!-- smooth scrolling js -->
    <script src="~/LayOuts/FullSite Files/js/SmoothScroll.min.js"></script>
    <!-- smooth scrolling js -->
    <!-- start-smooth-scrolling -->
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/move-top.js"></script>
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/easing.js"></script>
    <script type="text/javascript">
	jQuery(document).ready(function($) {
		$(".scroll").click(function(event){
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
		});
	});
    </script>

    <!-- here starts scrolling icon -->
    <script type="text/javascript">
		$(document).ready(function() {
			/*
				var defaults = {
				containerID: 'toTop', // fading element id
				containerHoverID: 'toTopHover', // fading element hover id
				scrollSpeed: 1200,
				easingType: 'linear'
				};
			*/

			$().UItoTop({ easingType: 'easeOutQuart' });

			});
    </script>
    <!-- //here ends scrolling icon -->
    <!-- move to top-js-files -->
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/move-top.js"></script>
    <script type="text/javascript" src="~/LayOuts/FullSite Files/js/easing.js"></script>
    <!-- //move to top-js-files -->

</body>
